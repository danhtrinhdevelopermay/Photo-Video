<?xml version="1.0" encoding="utf-8"?>
<!--
   Backup rules file for iOS Style Media Viewer
   See https://developer.android.com/guide/topics/data/autobackup
   for details.
   Note: This file is ignored on Android 12+ (API 31+) devices. Use data_extraction_rules.xml
         instead.
-->
<full-backup-content>
   <!-- Include shared preferences for app settings -->
   <include domain="sharedpref" path="."/>
   <!-- Exclude device-specific settings -->
   <exclude domain="sharedpref" path="device.xml"/>
   <!-- Exclude cache directories -->
   <exclude domain="file" path="cache"/>
</full-backup-content>